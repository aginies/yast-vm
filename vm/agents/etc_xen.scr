.etc.xen

`ag_ini(`IniAgent( [ "/etc/xen/*" ],
    $[
	"options" : [ "line_can_continue", "global_values", "join_multiline", "comments_last" ],
	"comments": [ "^[ \t]*#.*$", "#.*", "^[ \t]*$", ],
	"params" : [
	    $[
		"match" : [ "^[ \t]*([a-zA-Z0-9_]+)[ \t]*=[ \t]*(.*)", "%s = %s" ],
	    ],
	],
])
)
